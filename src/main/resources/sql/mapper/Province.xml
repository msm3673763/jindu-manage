<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ucsmy.jindu.manage.manage.dao.ProvinceMapper">

    <select id="getProvince"  resultType="com.ucsmy.jindu.manage.manage.entity.Province">
        SELECT
        t.id as id,
        t.province_no as provinceNo,
        t.province_name as provinceName
        from jindu_province t
    </select>
</mapper>